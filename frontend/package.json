{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint --ext .js,.ts,.vue src tests",
    "format": "prettier --write .",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "prepare": "cd .. && husky frontend/.husky"
  },
  "dependencies": {
    "@primeuix/themes": "^2.0.2",
    "@tailwindcss/vite": "^4.1.17",
    "@tato30/vue-pdf": "^1.11.5",
    "axios": "^1.13.2",
    "dompurify": "^3.3.1",
    "highlight.js": "^11.11.1",
    "marked": "^17.0.1",
    "pdf-lib": "^1.17.1",
    "primeflex": "^4.0.0",
    "primeicons": "^7.0.0",
    "primevue": "^4.5.2",
    "tailwindcss": "^4.1.17",
    "vue": "^3.5.25",
    "vue-router": "^4.6.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@eslint/json": "^0.14.0",
    "@playwright/test": "^1.57.0",
    "@primevue/auto-import-resolver": "^4.5.2",
    "@vitejs/plugin-vue": "^6.0.2",
    "@vitest/coverage-v8": "^4.0.15",
    "@vue/test-utils": "^2.4.6",
    "eslint": "^9.39.1",
    "eslint-plugin-vue": "^10.6.2",
    "globals": "^16.5.0",
    "husky": "^9.1.7",
    "jsdom": "^27.3.0",
    "lint-staged": "^16.2.7",
    "prettier": "3.7.4",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.49.0",
    "unplugin-vue-components": "^30.0.0",
    "vite": "^7.2.7",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-sitemap": "^0.8.2",
    "vite-plugin-vue-devtools": "^8.0.5",
    "vitest": "^4.0.15"
  },
  "lint-staged": {
    "*.{js,jsx,vue}": [
      "pnpm exec eslint --ext .js,.ts,.vue src tests",
      "pnpm exec prettier --write ."
    ]
  }
}
